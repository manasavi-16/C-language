<script>

var project_uid  = null; // Required for saveCode to work
var dt = null;

function move_snippet(snippet_uid, folder_id, callback){
    
    shareCode(false, {move_snippet: true, callback:callback,
        uid:snippet_uid,
    parent_id : (folder_id>0 ? folder_id: null)});
}

function get_parent_folder_id(){
    var folder_id = $("#folder_id").val();
    return folder_id;
}

$(document).ready(function() {
    render_ogdb_dates();
    $('[data-toggle="tooltip"]').tooltip();
    dt = $('#project_list').DataTable({paging: false, 
        rowReorder: {
        snapX: false,
    },"order": [[3,'desc']], "columns": [ { "orderable": false }, null, null,null,null,{ "orderable": false } ]});
    dt.on('row-reorder-dropped', function(e, details, edit){
        var moving_row = dt.row(edit.triggerRow);
        var drop_target_row = dt.row(edit.dropRow);
        var moving_row_id = moving_row.id();
        var drop_target_row_id = drop_target_row.id();
        //moving_row.remove();
        //dt.draw();
        //ajax api to move project/directory to directory

        var project_uid = $("#"+moving_row_id).attr("data-uid");
        var folder_id = $("#"+drop_target_row_id).attr("data-id");
        var on_snippet_moved = function(data){
            moving_row.remove();
            dt.draw();
        }
        move_snippet(project_uid, folder_id, on_snippet_moved);
    })

    
    dt.on('row-reorder-dropped-prj', function(e, details, edit){
        var moving_row = dt.row(edit.triggerRow);
        var prjObj = edit.prjObj;
        var moving_row_id = moving_row.id();
        var project_uid = $("#"+moving_row_id).attr("data-uid");
        var folder_id = prjObj.attr("data-id");
        console.log(project_uid, folder_id);
        var on_snippet_moved = function(data){
            moving_row.remove();
            dt.draw();
        }
        move_snippet(project_uid, folder_id, on_snippet_moved);
    });
    

    $("#btn_new_folder").on('click', on_new_folder);

    $("#project_name").keypress(function(event){
        var key = event.which;
        if(key == 13){
            event.preventDefault();
            shareCode(false, {new_folder: true, parent_id:get_parent_folder_id() , callback:on_project_added});
            return false;
        }
    });
});

function on_project_added(data){
    var row_template = $("#new_folder_row_template").html();
    var project = data;
    var html = tmpeng(row_template, {project_uid:data.uid, project_id: data.id, project_name:data.name});
    dt.row.add($(html )).draw(false);
}

function on_new_folder(){
    saveCode({new_folder: true, parent_id:get_parent_folder_id(), callback:on_project_added})
}

function get_ProjectUID(obj){
    return $(obj).parent().parent().attr('data-uid');
}
function get_ProjectName(obj){
    return $(obj).parent().parent().find('.project_name').text();
}
function set_ProjectName(obj, name){
    return $(obj).parent().parent().find('.project_name').text(name);
}

function handle_edit_file_input(e, opts){
    var pid = opts.pid;
    var obj = opts.obj;

    //console.log("handle_edit_file_input:");
    var filename = g_renamefilemodal_get_edit_filename();
    //console.log("new file name ", filename);
    $(obj).attr('disabled',true);
    $.ajax({
            type: 'POST',
            contentType: 'application/json',
            url: '/rename/'+pid+'?newfilename='+filename,
            success: function(data) {
                //console.log(data);
                if(data.result=="LOGIN_ERROR"){
                    resetLogin(); renameProject(obj);
                }else if(data.result=="OK"){
                    set_ProjectName(obj, filename);
                    $(obj).attr('disabled',false);
                }else{
                    $(obj).attr('disabled',false);
                }
            },
            error: function() {
                $(obj).attr('disabled',false);
            }
        });
    g_renamefilemodal_hide();
}
var rename_project_obj=null;
function rename_project_callback(){
    renameProject(rename_project_obj);
}
function renameProject(obj){
    var project_name = get_ProjectName(obj);
    var pid = get_ProjectUID(obj);
    rename_project_obj =  obj;
    if(popUpLogin("Please login to rename project", rename_project_callback)) return;
    g_renamefilemodal_popup({default_name:project_name, title:"Rename Project"},
    handle_edit_file_input,{pid:pid, obj:obj});
}

function requestToMoveProject(opts, callback){
    var parent_id = opts.parent_id;
    var prj_uid = opts.prj_uid;

    $.ajax({
        type: 'POST',
        data: JSON.stringify({parent_id:parent_id}),
        contentType: 'application/json',
        url: '/project/move/'+prj_uid,
        success: function(data) {
            generic_json_request_success_handler(data, callback);
        },
        error:function(){
            error_completing_action();
        }
    });
}

function moveProject(obj){
    chooseDestinationFolder(function(folder_id, folder_name){
        if(!folder_id) return;

        if(folder_id == get_current_folder_id()) {
            return;
        }
    
        var prj_uid = get_ProjectUID(obj);
        var project_name = get_ProjectName(obj);
        requestToMoveProject({parent_id: folder_id, prj_uid: prj_uid},
         function(){
            $(obj).parent().parent().remove();
            $.toaster({ message : "<b>"+project_name+"</b> moved to <b>"+folder_name+"</b>" });
        })
    })
}

function downloadProject(obj){
    $(obj).attr('disabled',true);
    var uid = get_ProjectUID(obj);
    Download('/download/'+uid);
    $(obj).attr('disabled',false);
}

function forkProject(obj){
    var uid = get_ProjectUID(obj);
    window.location.href="/fork/"+uid;
}

function shareProject(obj, snippet_type='S'){
    $(obj).attr('disabled',true);
    var uid = get_ProjectUID(obj);

    var msg = "Project is shared on below public link.<br\> Any changes made to your project will be visible in this link.";
    if(snippet_type == 'F') {
        msg = "Folder is shared on below publicly visible link. <br\> All project(s) and folder(s) under this shared folder would be accesible through link.<br\>"+
        " And any changes made to folder(s)/project(s) within would be visible to others as well.";
    }

    $.ajax({
            type: 'POST',
            contentType: 'application/json',
            url: '/shareProject/'+uid,
            success: function(data) {
                if(data.result=="LOGIN_ERROR"){
                    resetLogin(); shareProject(obj);
                }else if(data.result=="OK"){
                    share_code_snippet_modal(uid, snippet_type, msg);
                    $(obj).attr('disabled',false);
                }else{
                    $(obj).attr('disabled',false);
                }
            },
            error: function() {
                $(obj).attr('disabled',false);
            }
        });
}

function restoreProject(obj){
    deleteProject(obj, {restore:true});
}

function deleteProjectPermanently(obj){
    deleteProject(obj, {perm_delete:true});
}

var delete_project_obj = null;
var delete_project_opt = false;

function delete_project_callback(){
    deleteProject(delete_project_obj, delete_project_opt);
}
function deleteProject(obj, opt){
    delete_project_obj =  obj;
    delete_project_opt = opt;
    var perm_delete = opt?opt.perm_delete:false;
    var restore = opt?opt.restore:false;
    
    $(obj).attr('disabled',true);
    var uid = get_ProjectUID(obj);
    var project_name = get_ProjectName(obj);
    //console.log("Delete project");
    var popUpMsg = "Please login to delete project/folder";

    if(restore) popUpMsg = "Please login to restore project/folder";

    if(popUpLogin(popUpMsg, delete_project_callback)) return;

    var r = false;
    
    if(restore){ r = true;
    }else {
        r =confirm("Are you sure you want to delete project `"
    +project_name+"` "+(perm_delete?"permanently":"")+"?");
    }

    var params = "?";
    if(perm_delete) params +="&perm_delete=true";
    if(restore) params +="&restore=true";

    if (r == true) {
        $.ajax({
            type: 'POST',
            contentType: 'application/json',
            url: '/delete/'+uid+params,
            success: function(data) {
                console.log(data);
                if(data.result=="LOGIN_ERROR"){
                    resetLogin(); delete_project_callback();
                }else if(data.result=="OK"){
                    $(obj).parent().parent().remove();
                }else{
                    $(obj).attr('disabled',false);
                }
            },
            error: function() {
                $(obj).attr('disabled',false);
            }
        });
    }else{
        $(obj).attr('disabled',false);
    }
}
</script>
<script>
    function track_event(arg1, arg2){
        //if(typeof ga !== 'undefined' && ga) ga('send', 'event', arg1, arg2);
        if(gtag)
        gtag('event', arg2, {
        'event_category': arg1
        });
    }
    function is_mobile(){
        return $('#mobile_test_element').css('display')=='none';
    }

    (function () {
        if(no_ads) return;

        if (typeof _bsa !== 'undefined' && _bsa) {           
            if(!is_mobile()) {

            _bsa.init('custom', 'CK7I6K7M', 'placement:onlinegdbcom-top', {
                target: '#native-inline',
                template: `<a href="##link##" target="_blank" rel="noopener" rel="nofollow"><div class="sponsor">Sponsor</div><div class="native-description">##company## — ##description##</div></a>
                <a href="/upgrade-premium" target="_blank" type="button" class="close" data-dismiss="modal" style="color: white;opacity: 1;" title="Remove Ads">×</a>`
            });
            }
        } else {
            track_event('BSA Undefined', 'bsa_undefined');
        }
    })();

    function BSANativeCallback(data) {

        if (data.segment=="placement:onlinegdbcom-top") {
            if((data.ads && data.ads.length > 0))  {
                $("#native-inline").css('display', 'block');
                track_event('BSA Top AdUnit', 'bsa_top_adunit');
            } else {
                track_event('BSA No Top AdUnit', 'bsa_no_top_adunit');
            }
        }
    }
</script>


